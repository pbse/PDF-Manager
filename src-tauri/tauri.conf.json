{
  "productName": "pdf-manager",
  "version": "1.0.0",
  "build": {
    "beforeDevCommand": "npm run dev --prefix ./src/ui",
    "beforeBuildCommand": "npm run build --prefix ./src/ui",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../src/ui/build"
  },
  "identifier": "com.yourdomain.pdfmanager",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "width": 900,
        "height": 700,
        "resizable": true,
        "minWidth": 600,
        "minHeight": 500,
        "title": "PDF Manager"
      }
    ],
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "category": "Utility",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "plugins": {}
}